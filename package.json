{
  "name": "webcompat",
  "title": "Web Compat",
  "description": "Urgent post-release fixes for web compatibility.",
  "license": "MPL-2.0",
  "repository": "https://github.com/mozilla/webcompat-addon",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "jake": "npx jake",
